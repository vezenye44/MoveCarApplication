<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/imageView"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:rotation="180"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/step1">
        <Constraint
            android:id="@+id/imageView"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:rotation="210"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.32"
            motion:layout_constraintVertical_bias="0.26" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/step2" >
        <Constraint
            android:layout_width="96dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView"
            android:layout_height="96dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="180"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintVertical_bias="0.0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/step3" >
        <Constraint
            android:layout_width="96dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView"
            android:layout_height="96dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="90"
            motion:layout_constraintVertical_bias="0.26"
            motion:layout_constraintHorizontal_bias="0.82" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/step4" >
        <Constraint
            android:layout_width="96dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView"
            android:layout_height="96dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="360"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/step5" >
        <Constraint
            android:layout_width="96dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView"
            android:layout_height="96dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="290"
            motion:layout_constraintHorizontal_bias="0.61" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/step6" >
        <Constraint
            android:layout_width="96dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView"
            android:layout_height="96dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="180"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="1.0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/step7" >
        <Constraint
            android:layout_width="96dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView"
            android:layout_height="96dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="90"
            motion:layout_constraintVertical_bias="0.69"
            motion:layout_constraintHorizontal_bias="0.6" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/step8" >
        <Constraint
            android:layout_width="96dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView"
            android:layout_height="96dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="180"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/step9" >
        <Constraint
            android:layout_width="96dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView"
            android:layout_height="96dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="180" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/step10" >
        <Constraint
            android:layout_width="96dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView"
            android:layout_height="96dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="180" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/end" >
        <Constraint
            android:layout_width="96dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView"
            android:layout_height="96dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="180" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/step1" >
        <OnClick motion:targetId="@id/imageView" />
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/step1"
        motion:constraintSetEnd="@+id/step2"
        motion:autoTransition="animateToEnd" />
    <Transition
        motion:constraintSetStart="@+id/step2"
        motion:constraintSetEnd="@+id/step3" >
        <OnClick motion:targetId="@id/imageView" />
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/step3"
        motion:constraintSetEnd="@+id/step4"
        motion:autoTransition="animateToEnd" />
    <Transition
        motion:constraintSetStart="@+id/step4"
        motion:constraintSetEnd="@+id/step5" >
        <OnClick motion:targetId="@id/imageView" />
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/step5"
        motion:constraintSetEnd="@+id/step6"
        motion:autoTransition="animateToEnd" />
    <Transition
        motion:constraintSetStart="@+id/step6"
        motion:constraintSetEnd="@+id/step7">
        <OnClick motion:targetId="@id/imageView" />
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/step7"
        motion:constraintSetEnd="@+id/step8"
        motion:autoTransition="animateToEnd" />
    <Transition
        motion:constraintSetStart="@+id/step8"
        motion:constraintSetEnd="@+id/step9" >
        <OnClick motion:targetId="@id/imageView" />
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/step9"
        motion:constraintSetEnd="@+id/step10"
        motion:autoTransition="animateToEnd" />
    <Transition
        motion:constraintSetStart="@+id/step10"
        motion:constraintSetEnd="@+id/end"
        motion:autoTransition="animateToEnd"
        motion:duration="100" />
    <Transition
        motion:constraintSetStart="@+id/end"
        motion:constraintSetEnd="@+id/start"
        motion:autoTransition="jumpToEnd"
        motion:duration="100" />
</MotionScene>